{
  "name": "A-or-B",
  "version": "1.0.0",
  "description": "A simple game where you create a game with a title, and two buttons A or B that you can name whatever you want (f.ex. True and False) and then you can create a variable amount of rounds which are images or videos that are either A or B.",
  "main": "index.tsx",
  "scripts": {
    "lint": "eslint --ignore-path .eslintignore --ext js,jsx,ts,tsx .",
    "fix": "eslint --ignore-path .eslintignore --ext js,jsx,ts,tsx . --fix",
    "test": "jest",
    "validate": "npm run-script lint && npm run-script test",
    "build": "webpack -p --progress --config ./webpack.production.config.js",
    "watch": "webpack --progress -d --config ./webpack.development.config.js --watch",
    "start": "webpack-dev-server --config ./webpack.development.config.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run-script lint",
      "pre-push": "npm run-script validate"
    }
  },
  "author": "",
  "license": "",
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "@react-keycloak/web": "^2.1.1",
    "@types/styled-components": "^5.1.2",
    "history": "^4.10.1",
    "husky": "^4.0.10",
    "keycloak-js": "^10.0.2",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "styled-components": "^5.0.0",
    "styled-icons": "^9.2.0",
    "swr": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-destructuring": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.7.4",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.0.0",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/jest": "^26.0.13",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.3.0",
    "typescript": "^3.9.7",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  }
}
